{
  "project_name": "My MCP Server",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "description": "MCP server with decorators",
  "author_name": "Your Name",
  "author_email": "email@example.com",
  "python_version": ["3.11", "3.12"],
  "include_admin_ui": ["no", "yes"],
  "include_example_tools": ["yes", "no"],
  "include_parallel_example": ["yes", "no"],
  "mcp_config_file_path": "",
  "server_port": "3001",
  "default_transport": ["stdio", "sse", "streamable-http"],
  "streamable_http_enabled": ["yes", "no"],
  "streamable_http_endpoint": "/mcp",
  "streamable_http_json_response": ["no", "yes"],
  "log_level": ["INFO", "DEBUG", "WARNING", "ERROR"],
  "log_retention_days": "30",
  "_copy_without_render": [
    ".claude",
    ".reference/templates/*",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.gif",
    "*.ico"
  ]
}
